{
  "typeRoots": ["./typings"],
  "compilerOptions": {
    "outDir": "./dist/",
    "noImplicitAny": true,
    "sourceMap": true,
    "module": "esnext",
    "moduleResolution": "node",
    "target": "es5",
    "jsx": "react",
    "allowJs": true,
    "rootDir": ".",
    "allowSyntheticDefaultImports": true,
    "lib": ["es2015", "dom"]
  },
  "exclude": ["node_modules", "**/*.js"]
}
